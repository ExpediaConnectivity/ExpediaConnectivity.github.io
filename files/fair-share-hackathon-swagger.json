                {
                    "description": "Get the fair share data for the given stayDates",
                    "method": "GET",
                    "summary": "Get the fair share data for the given stayDates",
                    "notes": "The fairShare API projects a competitive metric that compares a given hotel's expected market share vs. their competitor's market share based on a given stayDate.  The equation is: myRoomCount / (myRoomCount + compSetRoomCount) to compute FairShare for a given hotel for a given staydate.",
                    "nickname": "fairShare",
                    "produces": [
                        "application/json"
                    ],
                    "type": "FairShareJSONResponse",
                    "parameters": [
                        {
                            "name": "hotelId",
                            "paramType": "query",
                            "description": "hotel ID",
                            "type": "integer"
                        },
                        {
                            "name": "dayNum",
                            "paramType": "query",
                            "description": "(Optional)the number of days from today to the future, default value is 1",
                            "type": "integer"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 3000, errorMsg: 'clientId is necessary.'"
                        },
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 3010, errorMsg: 'HotelId is necessary.'"
                        },
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 3011, errorMsg: 'The hotelId was not an integer.'"
                        },
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 3012, errorMsg: 'The hotelId should be positive integer.'"
                        },
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 2000, errorMsg: 'Hotel is not exist'"
                        },
                        {
                            "code": 400,
                            "message": "status: 'Error', errorCode: 2002, errorMsg: 'This hotel has no competitors'"
                        }
                    ]
                }
            ]
        }
    ],
